# docker shortcuts
docker-rm() { docker rm $(docker ps -aq); }
docker-rmi() { docker rmi $(docker images -f "dangling=true" -q); }
docker-run() { docker run -it --rm $1; }
docker-build() { docker build -t $1 .; }

